import java.util.*;
class java20
{
	public static void main(String[] args) 
	{
		Scanner input=new Scanner(System.in);
		System.out.println("Enter the number: ");
		int n=input.nextInt();
		int rem,r=0;
		while(n>0)
		{
			rem=n%10;
			r=r*10+rem;
			n=n/10;
		}
		System.out.println("The sum of digits is: "+r);
	}
}

