class Box
{
	private int width;
	private int height;
	private int depth;
	private int volume;
	Box(int width,int height,int depth)
	{
		super();
		this.width=width;
		this.height=height;
		this.depth=depth;
	}
	int getwidth()
	{
		return width;
	}
	int getheight()
	{
		return height;
	}
	int getdepth()
	{
		return depth;
	}
	int getvolume()
	{
		return width*height*depth;
	}
}
class java38 
{
	public static void main(String[] args) 
	{
		Box b=new Box(10,20,10);
		System.out.println("Volume of box: "+b.getvolume());

	}
}
